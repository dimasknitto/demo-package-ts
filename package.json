{
  "name": "demo-package-ts",
  "version": "1.3.4",
  "description": "testing",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "release": "release-it"
  },
  "keywords": [],
  "author": "dimas knitto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dimasknitto/demo-package-ts.git"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}",
      "push": true,
      "commit": true,
      "tag": true,
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}",
      "draft": true,
      "releaseNotes": "git log --no-merges --pretty=format:\"* %s %h\" ${latestTag}...main",
      "assets": [
        "dist/*.js"
      ]
    },
    "npm": {
      "publish": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md",
        "header": "# Changelog"
      }
    }
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^8.0.1",
    "release-it": "^17.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "demo-package-ts": "link:"
  }
}
